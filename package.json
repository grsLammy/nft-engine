{
  "name": "nft-engine-zkevm",
  "version": "1.0.0",
  "description": "simple NFT Engine smart contract for zkEVM",
  "scripts": {
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "deploy:zkEVM": "npx hardhat run --network zkEVM scripts/deploy_NFTEngine.ts",
    "issueBatch:zkEVM": "npx hardhat run --network zkEVM scripts/issueBatch.ts",
    "deploy:mumbai": "npx hardhat run --network mumbai scripts/deploy_NFTEngine.ts"
  },
  "author": "Gulam Rasul Shah",
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.1.0",
    "@typechain/hardhat": "^2.3.0",
    "@types/node": "^16.9.6",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.4",
    "hardhat": "^2.6.4",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "*.{ts,json,sol,md,yml}": "prettier --write"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.3.2",
    "dotenv": "^10.0.0",
    "prompt-sync": "^4.2.0"
  }
}
